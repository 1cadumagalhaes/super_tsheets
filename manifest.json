{
    "name": "Super TSheets",
    "version": "2.0",
    "description": "Extens√£o para ajudar a lidar com o tsheets",
    "manifest_version": 2,
    "host_permissions": [
        "https://tsheets.intuit.com"
    ],
    "browser_action": {
        "default_icon": {
            "32": "clock-icon.png"
        },
        "default_popup": "popup.html",
        "default_title": "Super TSheets"
    },
    "permissions": [
        "activeTab",
        "storage"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://dp6.tsheets.com/*",
                "http://dp6.tsheets.com/*",
                "https://tsheets.intuit.com/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}